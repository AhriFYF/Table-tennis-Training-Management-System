<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.system.tabletennis_training_management_system.mapper.UserMapper">
    <select id="listAll" resultType="com.system.tabletennis_training_management_system.pojo.User">
        select * from tb_table
    </select>

    <select id="findByUserNameLike" resultType="com.system.tabletennis_training_management_system.pojo.User">
        select * from tb_table where user_name like '%${userName}%'
    </select>
</mapper>